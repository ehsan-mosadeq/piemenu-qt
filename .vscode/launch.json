    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-win/Debug/RadialPopupMenu.exe",
            "MIMode": "gdb",
            "miDebuggerPath": "C:/Qt6/Tools/mingw1310_64/bin/gdb.exe",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "CMake: build",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${env:PATH};C:/Qt6/6.8.0/llvm-mingw_64/bin"
                }
            ],
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "visualizerFile": "c:\\Users\\ehsan\\AppData\\Roaming\\Code\\User\\workspaceStorage\\104d9e38a31d8436dfe70fc3be8fdb14\\tonka3000.qtvsctools\\qt.natvis.xml"
        }
    ]
}